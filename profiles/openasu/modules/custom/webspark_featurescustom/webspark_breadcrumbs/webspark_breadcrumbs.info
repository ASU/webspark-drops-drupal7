name = Webspark breadcrumbs
description = Adds easy_breadcrumbs; configures content page panel with title, breadcrumb and moscone default layout
core = 7.x
package = ASU Webspark
version = 7.x-1.0-dev
project = webspark_breadcrumbs
dependencies[] = ctools
dependencies[] = defaultconfig
dependencies[] = easy_breadcrumb
dependencies[] = features
dependencies[] = panelizer
dependencies[] = panopoly_pages
dependencies[] = strongarm
features[ctools][] = strongarm:strongarm:1
features[defaultconfig][] = strongarm:easy_breadcrumb-capitalizator_ignored_words
features[defaultconfig][] = strongarm:easy_breadcrumb-capitalizator_mode
features[defaultconfig][] = strongarm:easy_breadcrumb-disable_drupal_breadcrumb
features[defaultconfig][] = strongarm:easy_breadcrumb-home_segment_title
features[defaultconfig][] = strongarm:easy_breadcrumb-include_home_segment
features[defaultconfig][] = strongarm:easy_breadcrumb-include_invalid_paths
features[defaultconfig][] = strongarm:easy_breadcrumb-include_title_segment
features[defaultconfig][] = strongarm:easy_breadcrumb-segments_separator
features[defaultconfig][] = strongarm:easy_breadcrumb-title_from_page_when_available
features[defaultconfig][] = strongarm:easy_breadcrumb-title_segment_as_link
features[features_api][] = api:2
features[variable][] = panels_page_default
features_exclude[dependencies][webspark_extras] = webspark_extras
