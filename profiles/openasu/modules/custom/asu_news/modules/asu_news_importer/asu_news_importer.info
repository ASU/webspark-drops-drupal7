name = ASU News Importer
description = Import ASU News via Feeds.
core = 7.x
; Added to staging module package 
package = "ASU Staging"
dependencies[] = asu_feeds2
dependencies[] = ctools
dependencies[] = defaultconfig
dependencies[] = features
dependencies[] = filter
features[ctools][] = feeds:feeds_importer_default:1
features[ctools][] = feeds_tamper:feeds_tamper_default:2
features[ctools][] = strongarm:strongarm:1
features[defaultconfig][] = strongarm:pathauto_node_asu_news_pattern
features[defaultconfig][] = user_default_permissions:use text format asu_feeds_input_format
features[features_api][] = api:2
features[feeds_importer][] = asu_news_importer
features[feeds_tamper][] = asu_news_importer-xpathparser_1-html_entity_decode
features[feeds_tamper][] = asu_news_importer-xpathparser_1-html_entity_decode_1
features[feeds_tamper][] = asu_news_importer-xpathparser_28-asu_feeds2_timezone
features[feeds_tamper][] = asu_news_importer-xpathparser_28-strtotime
features[filter][] = asu_feeds_input_format
features[user_permission][] = use text format asu_feeds_input_format
features[variable][] = pathauto_node_asu_news_pattern
features_exclude[dependencies][defaultconfig] = defaultconfig
features_exclude[dependencies][feeds] = feeds
features_exclude[dependencies][feeds_tamper] = feeds_tamper
features_exclude[dependencies][feeds_xpathparser] = feeds_xpathparser
features_exclude[dependencies][strongarm] = strongarm
