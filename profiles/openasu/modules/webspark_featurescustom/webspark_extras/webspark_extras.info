name = Webspark Extras
description = "Contains miscellaneous Webspark customizations/settings that do not clearly fit elsewhere."
; Force SSL/HTTPS on Pantheon, Responsive image display, default admin theme, help link,
; cleanup content page, accordion settings, video help text.
core = 7.x
package = ASU Webspark
version = 7.x-1.8
project = webspark_extras
required = TRUE
dependencies[] = ctools
dependencies[] = defaultconfig
dependencies[] = defaultcontent
dependencies[] = entity
dependencies[] = entity_view_mode
dependencies[] = features
dependencies[] = file_entity
dependencies[] = menu
dependencies[] = strongarm
features[content_menu_links][] = management_help:webspark-help
features[ctools][] = file_entity:file_default_displays:1
features[ctools][] = panelizer:panelizer:1
features[ctools][] = strongarm:strongarm:1
features[defaultconfig][] = strongarm:admin_theme
features[defaultconfig][] = strongarm:node_admin_theme
features[features_api][] = api:2
features[file_display][] = image__responsive_image__file_field_image
features[menu_custom][] = management
features[variable][] = entity_view_modes
features_exclude[dependencies][panopoly_pages] = panopoly_pages
features_exclude[dependencies][panelizer] = panelizer
features_exclude[panelizer_defaults][node:panopoly_page:default:default] = node:panopoly_page:default:default
features_exclude[panelizer_defaults][node:panopoly_page:default:featured] = node:panopoly_page:default:featured
features_exclude[panelizer_defaults][node:panopoly_page:default:teaser] = node:panopoly_page:default:teaser
