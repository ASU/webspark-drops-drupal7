<?php

/**
 * @file
 * Install, update and uninstall functions for the frameprevention module.
 */

/**
 * Implements hook_uninstall().
 */
function frameprevention_uninstall() {
  variable_del('frameprevention_enabled');
  variable_del('frameprevention_x_frame_options');
  variable_del('frameprevention_pages');
}
